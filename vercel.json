{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/search/(.*)",
      "dest": "/api/search.js?query=$1"
    },
    {
      "src": "/ytmp3",
      "dest": "/api/ytmp3.js"
    },
    {
      "src": "/ytmp4",
      "dest": "/api/ytmp4.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}
